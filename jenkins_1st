pipeline{
  agent {
     label 'none'
  }
  stages {
      stage('1st') {
          agent{
            label 'master'
          }
          steps{
            sh "echo 1st"
          }
        
       }
      stage('2nd') {
          steps{
            sh "echo 1st"
          }
        
       }
      stage('3rd') {
          agent{
            label 'slave1'
          }
          steps{
            sh " sleep 10"
          }
        
       }
    }
}